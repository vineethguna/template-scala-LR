{
  "name": "pio-engine-heroku",
  "description": "Heroku compatible PredictionIO Engine",
  "keywords": [
    "machinelearning",
    "predictionio"
  ],
  "repository": "https://github.com/jamesward/pio-engine-heroku",
  "env": {
    "ACCESS_KEY": {
      "description": "PredictionIO Event Server App Access Key"
    },
    "APP_NAME": {
      "description": "PredictionIO Event Server App Name"
